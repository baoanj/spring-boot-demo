<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.baoanj.provider.mapper.BookMapper">
    <insert id="addBook">
        insert into books (book_id, book_name, book_page)
        values (#{bookId}, #{bookName}, #{bookPage})
    </insert>
</mapper>